private static readonly string[] OtherDocumentPhrases =
{
    "Zgoda umowy poręczenia",
    "Aneks umowy poręczenia",
    "Oświadczenie poręczyciela"
};

private static readonly string[] GuaranteeOrBillPhrases =
{
    "Weksel własny",
    "Oświadczenie wekslowe",
    "Deklaracja wekslowa BGK"
};

public bool AnyOtherDocument(List<List<string>> documents)
{
    return ContainsDocumentWithAnyPhrase(documents, OtherDocumentPhrases);
}

public bool AnyGuaranteeOrBillDocument(List<List<string>> documents)
{
    return ContainsDocumentWithAnyPhrase(documents, GuaranteeOrBillPhrases);
}

private static bool ContainsDocumentWithAnyPhrase(
    List<List<string>> documents,
    IEnumerable<string> phrases)
{
    if (documents == null || documents.Count == 0)
    {
        return false;
    }

    // Flatten all strings from inner lists and check if any cell contains any phrase
    return documents
        .Where(row => row != null)
        .SelectMany(row => row)
        .Where(cell => !string.IsNullOrWhiteSpace(cell))
        .Any(cell => phrases.Any(phrase =>
            cell.Contains(phrase, StringComparison.OrdinalIgnoreCase)));
}
