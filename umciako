// Zakładam, że masz model danych np. BalanceModel
private mapFormToModel(): BalanceModel {
  const formValue = this.formGroup.value;

  // Tworzymy nowy obiekt modelu, aby nie modyfikować starego przez referencję
  const model: BalanceModel = {
    productCode: formValue.productCode || null,
    selectedContract: formValue.selectedContract || null,
    lunchCreditDate: formValue.lunchCreditDate || null,
    supervisor: {
      name: formValue.supervisorName || null,
      surname: formValue.supervisorSurname || null,
      postCode: formValue.supervisorPostCode || null,
      city: formValue.supervisorCity || null
    },
    selectedDocument: formValue.selectedDocument || null,
    barcodeId: formValue.barcodeId || null,
    reportingReceivablesToIng: formValue.reportingReceivablesToIng || false
  };

  return model;
}
